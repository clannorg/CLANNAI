    "PM06": "Player Counts High Speed Running",
    "PM05": "Player Counts Jogging",
    "PM02": "Player Counts Running",
    "PM04": "Player Counts Sprinting",
    "PM01": "Player Counts Standing",
    "PM03": "Player Counts Walking",
    "PM17": "Player Distance Accelerating",
    "PM25": "Player Distance High Speed Running",
    "PM15": "Player Distance Jogging",
    "PM20": "Player Distance Running",
    "PM22": "Player Distance Sprinting",
    "PM11": "Player Distance Standing",
    "PM14": "Player Distance Walking",
    "PM13": "Player Time Accelerating",
    "PM26": "Player Time Decelerating",
